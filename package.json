{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "managing documents, roles and privledges",
  "main": "server/config/server.js",
  "scripts": {
    "test": "npm run test-build && NODE_ENV=test PORT=4000 nyc --reporter=text mocha --timeout 10000 --compilers js:babel-register -- ./server/tests/**/*.spec.js",
    "start": "npm run build",
    "build": "webpack -w & babel-node -- server/config/server.js & nodemon server/config/server.js",
    "rebuild:testdb": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "seed:testdb": "NODE_ENV=test sequelize db:seed:all",
    "test-build": "npm run rebuild:testdb && npm run seed:testdb"
  },
